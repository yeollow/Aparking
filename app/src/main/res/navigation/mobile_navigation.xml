<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.aparking.ui.home.HomeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/activity_bookmark"
        />

    <fragment
        android:id="@+id/nav_mypage"
        android:name="com.example.aparking.ui.mypage.MypageFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_mypage" />
    <fragment
        android:id="@+id/nav_bookmark"
        android:name="com.example.aparking.ui.BookmarkFragment"
        android:label="@string/app_name"
        tools:layout="@layout/activity_bookmark" />

    <fragment
        android:id="@+id/nav_reservationInfo"
        android:name="com.example.aparking.ui.reservation.ReservationInfo"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_mypage" />

    <fragment
        android:id="@+id/nav_payInfo"
        android:name="com.example.aparking.ui.pay.PayInfoFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_payinfo" />
    <fragment
        android:id="@+id/nav_consultant"
        android:name="com.example.aparking.ui.consultant.ConsultantFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_consultant" />
</navigation>